package midlevel;

public class Member {

	private int id;
	private String name;
	private int point;
	private double bonus;
	

	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getPoint() {
		return point;
	}
	public void setPoint(int point) {
		this.point = point;
	}

	public double getBonus() {
		return point * 0.03;
	}
		
}
