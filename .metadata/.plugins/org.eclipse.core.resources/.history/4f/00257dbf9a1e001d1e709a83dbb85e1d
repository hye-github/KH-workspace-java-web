
public class Calc {

	public int plus(int num1, int num2) {
	return num1 + num2;
	}
	
	public static int minus(int num1, int num2) {
		return num1 - num2;
	}
	public int mply(int num1, int num2) {
		return num1 * num2;
	}

	public static double divide(int num1, int num2) {
		return num1 / (double)num2;
	}

}
