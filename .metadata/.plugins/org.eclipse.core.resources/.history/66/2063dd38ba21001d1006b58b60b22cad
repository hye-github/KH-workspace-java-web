package midlevel;

import java.util.ArrayList;

public class Manager { // 설계도
	
		private ArrayList members= new ArrayList(); // import 해야함
		
	
		private int index = 0;


		public void addMember(Member m) {
			members.add(m);
		}
		
		public ArrayList getMembers() {
			return members;
		}
		
		public int getIndex() {
			return members.size();
		}
		
}
