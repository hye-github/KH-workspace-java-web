package midlevel;

public class Silver {

	private int id;
	private String name;
	private int point;
	private int bonus;


	public Silver() {}

	public Silver(int id, String name, int point) {
		this.id = id;
		this.name = name;
		this.point = point;
	}

	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getPoint() {
		return point;
	}
	public void setPoint(int point) {
		this.point = point;
	}

	public int getBonus() {
		return point/100;
	}
	public void setBonus(int bonus) {
		this.bonus = bonus;
	}
	

}
