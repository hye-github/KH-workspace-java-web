
public class Calc {

	public int a;
	public int b;
	
	

	public plus(int a, int b) {
	return a + b;
	}
	
	public static minus(int a, int b) {
		return a - b;
	}
	public mply(int a, int b) {
		return a * b;
	}

	public static divide(int a, int b) {
		return a / (double)b;
	}

	System.out.println(c.plus(10,20));		// 30
	System.out.println(calc.minus(30,20));	// 10
	System.out.println(c.mply(5,3));		// 15
	System.out.println(calc.divide(10,2));	// 5

}
